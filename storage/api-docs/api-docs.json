{
    "openapi": "3.0.0",
    "info": {
        "title": "APP SET V2",
        "description": "APP SET V2 Documentation",
        "contact": {
            "email": "alonso@setpay.mx"
        },
        "version": "2.0.0"
    },
    "paths": {
        "/api/reports/card-cloud/daily-consume": {
            "get": {
                "tags": [
                    "Reports"
                ],
                "summary": "Get daily consume from card cloud",
                "description": "Get daily consume from card cloud",
                "operationId": "dailyConsume",
                "parameters": [
                    {
                        "name": "date",
                        "in": "query",
                        "description": "Date to get the daily consume",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "format": "Y-m-d"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Daily consume from card cloud",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "total_amount": {
                                            "type": "number",
                                            "example": "1000.00"
                                        },
                                        "request_date": {
                                            "type": "string",
                                            "format": "Y-m-d",
                                            "example": "2025-01-01"
                                        },
                                        "movements": {
                                            "type": "array",
                                            "items": {
                                                "properties": {
                                                    "enviroment": {
                                                        "type": "string",
                                                        "example": "SET"
                                                    },
                                                    "company": {
                                                        "type": "string",
                                                        "example": "Company Name"
                                                    },
                                                    "client_id": {
                                                        "type": "string",
                                                        "example": "SP0000001"
                                                    },
                                                    "masked_pan": {
                                                        "type": "string",
                                                        "example": "516152XXXXXX2992"
                                                    },
                                                    "type": {
                                                        "type": "string",
                                                        "example": "Purchase"
                                                    },
                                                    "description": {
                                                        "type": "string",
                                                        "example": "Purchase description"
                                                    },
                                                    "amount": {
                                                        "type": "number",
                                                        "example": "100.00"
                                                    },
                                                    "authorization_code": {
                                                        "type": "string",
                                                        "example": "123456"
                                                    },
                                                    "date": {
                                                        "type": "string",
                                                        "format": "date-time",
                                                        "example": "2025-01-01 00:00:00"
                                                    }
                                                },
                                                "type": "object"
                                            }
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Error getting daily consume from card cloud",
                        "content": {
                            "text/plain": {
                                "schema": {
                                    "type": "string",
                                    "example": "Error getting daily consume from card cloud"
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        },
        "/api/commission/pay": {
            "post": {
                "tags": [
                    "Commissions"
                ],
                "summary": "Paga las comisiones de una cuenta",
                "description": "Paga las comisiones de una cuenta",
                "operationId": "payCommission",
                "requestBody": {
                    "description": "Datos de la cuenta",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "account"
                                ],
                                "properties": {
                                    "account": {
                                        "type": "string",
                                        "format": "string",
                                        "example": "123456789012345678"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Pago de comisión enviado",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Se ha enviado la orden de pago de comisión correctamente. Por favor espere la confirmación de STP."
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Error al pagar la comisión",
                        "content": {
                            "text/plain": {
                                "schema": {
                                    "type": "string",
                                    "example": "Error al pagar la comisión. Por favor intente más tarde."
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        }
    },
    "tags": [
        {
            "name": "Reports",
            "description": "Reports"
        },
        {
            "name": "Commissions",
            "description": "Commissions"
        }
    ]
}